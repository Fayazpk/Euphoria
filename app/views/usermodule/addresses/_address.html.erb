<div id="<%= dom_id address %>">
  <p>
    <strong>First name:</strong>
    <%= address.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= address.last_name %>
  </p>

  <p>
    <strong>User:</strong>
    <%= address.user_id %>
  </p>

  <p>
    <strong>Building:</strong>
    <%= address.building_name %>
  </p>

  <p>
    <strong>Street:</strong>
    <%= address.street_address %>
  </p>

  <p>
    <strong>City:</strong>
    <%= address.city.name %>
</p>
<p>
    <strong>State:</strong>
    <%= address.state.name %>
</p>
<p>
    <strong>Country:</strong>
    <%= address.country.name %>
</p>

  <p>
    <strong>Phone:</strong>
    <%= address.phone %>
  </p>

</div>
